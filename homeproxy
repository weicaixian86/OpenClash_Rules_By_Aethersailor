config ruleset 'cnip'
	option label 'cnip'
	option enabled '1'
	option type 'remote'
	option format 'binary'
	option url 'https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo-lite/geoip/cn.srs'
	option outbound '自动选择'

config ruleset 'cnsite'
	option label 'cnsite'
	option enabled '1'
	option type 'remote'
	option format 'binary'
	option url 'https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo-lite/geosite/cn.srs'
	option outbound '自动选择'

config dns_server 'google'
	option label 'google'
	option enabled '1'
	option address 'https://dns.google/dns-query'
	option address_resolver 'default-dns'
	option address_strategy 'ipv4_only'
	option resolve_strategy 'ipv4_only'
	option client_subnet '111.58.55.5'

config dns_rule 'node'
	option label 'node'
	option enabled '1'
	option mode 'default'
	list outbound 'any'
	option server 'default-dns'

config dns_rule 'clashdirect'
	option label 'clashdirect'
	option enabled '1'
	option mode 'default'
	option clash_mode 'direct'
	option server 'default-dns'

config dns_rule 'clashglobal'
	option label 'clashglobal'
	option enabled '1'
	option mode 'default'
	option clash_mode 'global'
	option server 'google'

config dns_rule 'sitecn'
	option label 'sitecn'
	option enabled '1'
	option mode 'default'
	list rule_set 'cnsite'
	option server 'default-dns'

config routing_rule 'direct'
	option label 'direct'
	option enabled '1'
	option mode 'default'
	option source_ip_is_private '0'
	option ip_is_private '0'
	option clash_mode 'direct'
	option rule_set_ipcidr_match_source '0'
	option outbound 'direct-out'

config routing_rule 'global'
	option label 'global'
	option enabled '1'
	option mode 'default'
	option source_ip_is_private '0'
	option ip_is_private '0'
	option clash_mode 'global'
	option rule_set_ipcidr_match_source '0'
	option outbound 'GLOBAL'

